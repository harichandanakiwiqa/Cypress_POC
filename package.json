{
  "name": "cypress_api",
  "version": "1.0.0",
  "description": "This project demonstrates how to automate the login process for the Sauce Labs demo website using Cypress. The code is written in JavaScript and uses the Mocha framework for testing.",
  "main": "index.js",
  "scripts": {
    "cypress:open": "npx cypress open --env configFile=qa --browser chrome",
    "cypress:run": "npx cypress run --env configFile=qa --browser chrome",
    "allure:report": "allure generate allure-results --clean -o allure-report",
    "allure:clear": "if exist allure-results rmdir /q /s allure-results && if exist allure-report rmdir /q /s allure-report && if exist cypress\\screenshots rmdir /q /s cypress\\screenshots && if exist cypress\\videos rmdir /q /s cypress\\videos",
    "pretest": "npm run allure:clear",
    "test": "npm run test || npm run posttest",
    "posttest": "npm run allure:report"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.17",
    "@shelex/cypress-allure-plugin": "^2.40.1",
    "allure-commandline": "^2.25.0",
    "babel-plugin-istanbul": "^6.1.1",
    "cypress": "^13.6.2",
    "cypress-mochawesome-reporter": "^3.8.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "istanbul-lib-coverage": "^3.2.2",
    "mocha-allure-reporter": "^1.4.0",
    "nyc": "^15.1.0",
    "prettier": "^3.1.1",
    "sinon-chai": "^3.7.0"
  },
  "dependencies": {
    "@cypress/instrument-cra": "^1.4.0",
    "chai": "^4.3.10",
    "jquery": "^3.7.1",
    "mocha": "^10.2.0",
    "save-dev": "^0.0.1-security",
    "sinon": "^17.0.1"
  }
}
